    <script>
    $('div.pace').empty().removeClass('pace pace-active')
      
    $(document).ready(function () {
        function updateCartDisplay() {
            $.ajax({
                url: '@Url.Action("calcart", "custermor")',
                type: 'GET',
                success: function (data) {
                    $('.cart .c__one span').text('$' + data.subtotal.toFixed(2));
                
                },
                error: function () {
                    console.error('Failed to fetch the cart totals.');
                }
            });
        }

        // Call the function to update the cart display when the page loads
        updateCartDisplay();

        // Optionally, call this function whenever the cart is updated
        // Example: after adding an item to the cart
        // updateCartDisplay();
    });

    </script>